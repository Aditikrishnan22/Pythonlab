n1=int(input("Enter the limit of 1st list:"))
a1=[]
for i in range(0,n1):
    b1=int(input("Enter element to input:"))
    a1.insert(i,b1)
print("LIST")
print(a1)
n2=int(input("Enter the limit of 2nd list:"))
a2=[]
for i in range(0,n2):
    b2=int(input("Enter element to input:"))
    a2.insert(i,b2)
print("LIST")
print(a2)
print("   LIST AFTER CONCATENATION ")
print(a1+a2)
